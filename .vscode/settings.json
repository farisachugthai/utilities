{
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "gitlens.advanced.telemetry.enabled": false,
    "python.pythonPath": "C:\\tools\\miniconda3\\envs\\utilities\\python.exe",
    "python.dataScience.exportWithOutputEnabled": true,
    "python.dataScience.sendSelectionToInteractiveWindow": true,
    "python.formatting.provider": "yapf",
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        "./test",
        "-p",
        "test*.py"
    ],
    "python.testing.pytestEnabled": false,
    "python.testing.nosetestsEnabled": false,
    "python.testing.unittestEnabled": true,
    "files.exclude": {
        "**/.git": false,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "files.eol": "\n",
    "editor.fontLigatures": true,
    "restructuredtext.experimental": true,
    "restructuredtext.languageServer.trace.server": "messages",
    "restructuredtext.confPath": "${workspaceFolder}/docs",
    "editor.fontFamily": "'Hack', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "restructuredtext.builtDocumentationPath": "${workspaceFolder}/docs/_build/html",
    "restructuredtext.languageServer.disabled": false,
    "docker.attachShellCommand.windowsContainer": "pwsh",
    "git.autofetch": true,
    "git.autoStash": true,
    "git.fetchOnPull": true,
    "gitlens.codeLens.includeSingleLineSymbols": true,
    "python.autoComplete.addBrackets": true,
    "python.dataScience.autoPreviewNotebooksInInteractivePane": true,
    "python.linting.flake8Enabled": true,
    "python.linting.pydocstyleEnabled": true,
    "python.terminal.executeInFileDir": true,
    "python.linting.pylintEnabled": false,
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "npm.packageManager": "yarn",
    "json.trace.server": "verbose",
    "vim.easymotion": true,
    "vim.enableNeovim": true,
    "vim.foldfix": true,
    "vim.hlsearch": true,
    "vim.highlightedyank.enable": true,
    "vim.leader": "<Space>",
    "vim.statusBarColorControl": true,
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "vim.sneak": true,
    "vim.useSystemClipboard": true,
    "vim.visualstar": true,
    "vsicons.presets.tsOfficial": true,
    "vsicons.presets.nestjs": true,
    "vsicons.presets.jsonOfficial": true,
    "vsicons.presets.jsOfficial": true,
    "vsicons.presets.angular": true,
    "yaml.format.enable": true,
    "emmet.triggerExpansionOnTab": true,
    "emmet.showSuggestionsAsSnippets": true,
}